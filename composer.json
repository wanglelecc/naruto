{
    "name": "wanglelecc/phpconsole",
    "description": "An object-oriented multi process manager for PHP",
    "type": "project",
    "homepage": "https://www.laracms.cn/",
    "license": "MIT",
    "authors": [
        {
            "name": "wanglelecc",
            "email": "wanglelecc@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1.3"
    },
    "autoload": {
        "classmap": [

        ],
        "psr-4": {
            "App\\": "./app",
            "Console\\": "./src"
        },
        "files" : [
        ]
